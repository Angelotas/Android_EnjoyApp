<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ngel.enjoyapp">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/juggling"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IntroActivity"
            android:screenOrientation="portrait"
            android:label="@string/labelMagia"
            android:theme="@style/Theme.AppCompat.CompactMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GridActivity" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity android:name=".SettingsActivity" android:theme="@style/Theme.AppCompat.DayNight"/>
        <activity android:name=".Result1Activity"
            android:label="@string/labelMagia"
            android:screenOrientation="portrait"/>
    </application>

</manifest>